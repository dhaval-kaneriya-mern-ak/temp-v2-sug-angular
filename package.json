{
  "name": "@sug-angular/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "postinstall": "node scripts/install-git-hooks.js",
    "uninstall-hooks": "node scripts/uninstall-git-hooks.js",
    "dev": "nx serve host --devRemotes=messages",
    "dev:host": "nx serve host --skipRemotes=messages",
    "dev:messages": "nx serve messages",
    "dev:reports": "nx serve reports",
    "dev:groups": "nx serve groups",
    "dev:all": "concurrently \"npm run dev:host\" \"npm run dev:messages\" \"npm run dev:groups\"",
    "dev:host:https": "nx serve host --configuration=https --skipRemotes=messages",
    "dev:messages:https": "nx serve messages --configuration=https",
    "dev:reports:https": "nx serve reports --configuration=https",
    "dev:groups:https": "nx serve groups --configuration=https",
    "dev:all:https": "concurrently \"npm run dev:host:https\" \"npm run dev:messages:https\" \"npm run dev:groups:https\"",
    "build": "nx build host",
    "build:host": "nx build host",
    "build:messages": "nx build messages",
    "build:reports": "nx build reports",
    "build:groups": "nx build groups",
    "build:all": "nx run-many -t build",
    "test": "nx run-many -t test",
    "test:host": "nx test host",
    "test:messages": "nx test messages",
    "test:reports": "nx test reports",
    "test:groups": "nx test groups",
    "test:ci": "nx run-many -t test --ci --code-coverage",
    "lint": "nx run-many -t lint",
    "lint:host": "nx lint host",
    "lint:messages": "nx lint messages",
    "lint:reports": "nx lint reports",
    "lint:groups": "nx lint groups",
    "lint:fix": "nx run-many -t lint --fix",
    "format": "prettier --check apps/",
    "format:fix": "prettier --write apps/",
    "format:all": "npm run format:fix && npm run lint:fix",
    "check:all": "npm run format && npm run lint",
    "affected:build": "nx affected -t build",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint",
    "clean": "nx reset",
    "dep-graph": "nx dep-graph",
    "start": "npm run dev",
    "preview": "nx preview host",
    "preview:all": "concurrently \"npx http-server dist/apps/host -p 4200 --cors -P http://localhost:4200?\" \"npx http-server dist/apps/messages -p 4202 --cors -P http://localhost:4202?\"",
    "e2e": "nx run-many -t e2e",
    "generate:component": "nx g @nx/angular:component",
    "generate:service": "nx g @nx/angular:service",
    "ci:build": "npm run build:all",
    "ci:lint": "npm run lint",
    "ci:all": "npm run ci:lint && npm run ci:build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.6",
    "@angular/common": "20.1.6",
    "@angular/compiler": "20.1.6",
    "@angular/core": "20.1.6",
    "@angular/forms": "20.1.6",
    "@angular/localize": "20.1.6",
    "@angular/platform-browser": "20.1.6",
    "@angular/router": "20.1.6",
    "@lumaverse/sug-ui": "1.8.1",
    "@ng-bootstrap/ng-bootstrap": "19.0.1",
    "@nx/angular": "21.3.11",
    "@primeng/themes": "20.0.1",
    "bootstrap": "5.3.8",
    "chart.js": "4.5.1",
    "date-fns": "4.1.0",
    "ngx-captcha": "14.0.0",
    "ngx-toastr": "19.1.0",
    "primeicons": "7.0.0",
    "primeng": "20.0.1",
    "rxjs": "7.8.2",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "~1.19.1",
    "@analogjs/vitest-angular": "~1.19.1",
    "@angular-devkit/build-angular": "20.1.5",
    "@angular-devkit/core": "20.1.5",
    "@angular-devkit/schematics": "20.1.5",
    "@angular/build": "20.1.5",
    "@angular/cli": "20.1.5",
    "@angular/compiler-cli": "20.1.6",
    "@angular/language-service": "20.1.6",
    "@eslint/js": "^9.8.0",
    "@module-federation/enhanced": "0.17.1",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/module-federation": "21.3.11",
    "@nx/vite": "21.3.11",
    "@nx/web": "21.3.11",
    "@nx/webpack": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@schematics/angular": "20.1.5",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.29.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "angular-eslint": "^20.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "lint-staged": "^16.1.5",
    "nx": "21.3.11",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "lint-staged": {
    "apps/**/*.{ts,js}": [
      "prettier --check",
      "eslint --config eslint.config.mjs"
    ],
    "apps/**/*.{html,scss,css}": [
      "prettier --check"
    ]
  },
  "overrides": {
    "@module-federation/node": "2.7.12",
    "@module-federation/enhanced": "0.17.1",
    "@rspack/core": "1.4.11",
    "webpack": "5.99.9"
  }
}
